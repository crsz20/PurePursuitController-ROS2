<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>nova_motion_control</name>
  <version>0.0.0</version>
  <description>A controls node that interfaces with a coupled TBS (throttle+braking+speed) controller. By Nova at UTD.</description>
  <maintainer email="Cristian.CruzLopez@utdallas.edu">clopez</maintainer>
  <license>MIT License</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_auto</buildtool_depend>


  <build_depend>nav_msgs</build_depend>
  <depend>autoware_auto_msgs</depend>
  <depend>voltron_msgs</depend>
  <depend>rclcpp</depend>
  <depend>visualization_msgs</depend>
  <depend>geometry_msgs</depend>



  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>
  <test_depend>ament_lint_auto</test_depend>


  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
